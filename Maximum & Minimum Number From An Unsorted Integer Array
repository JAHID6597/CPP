#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    cin>>n;

    int mx = -1;
    int mn = INT_MAX;

    for(int i=0;i<n;i++){
        int x;
        cin>>x;
        mx = max(mx, x);
        mn = min(mn, x);
    }

    cout<<"Maximum Number: "<<mx<<endl;
    cout<<"Minimum Number: "<<mn<<endl;

    return 0;
}
